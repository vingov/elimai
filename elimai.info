name = Elimai
description = Elimai means Simplicity in Tamil Language. This is a simple responsive Drupal 7 theme based on the Twitter Bootstrap.
core = 7.x

; Stylesheets
stylesheets[all][] = css/style.css
stylesheets[all][] = css/responsive.css

; JavaScript files
scripts[] = js/script.js

; Regions
regions[header] = Header
regions[help] = Help
regions[highlighted] = Highlighted
regions[content] = Content
regions[sidebar_first] = Sidebar
regions[footer] = Footer

; Settings
settings[bootstrap_source] = bootstrapcdn
settings[bootstrap_version] = 2.3.2
settings[breadcrumb_delimiter] = ' Â» '
settings[social_block] = 1
settings[facebook_url] = "https://www.facebook.com"
settings[twitter_url] = "https://twitter.com"
settings[gplus_url] = "https://plus.google.com"
settings[pinterest_url] = "http://pinterest.com"
